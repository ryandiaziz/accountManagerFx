   <children>
      <TableView fx:id="tableViewHistory" layoutY="73.0" prefHeight="327.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="columnChangedAt" prefWidth="140.0" text="Tanggal Diubah" />
          <TableColumn fx:id="columnAccountName" prefWidth="153.0" text="Akun" />
            <TableColumn fx:id="columnOldPassword" prefWidth="153.0" text="Password Lama" />
            <TableColumn fx:id="columnNewPassword" prefWidth="153.0" text="Password Baru" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="34.0" mnemonicParsing="false" onAction="#handleBack" text="Kembali" />
   </children>
</AnchorPane>
